{
  "name": "starcomgame.com",
  "version": "0.9.4",
  "scripts": {
    "postinstall": "elm-tooling install",
    "dev": "iles dev --open",
    "build": "iles build",
    "prebuild": "elm-tooling install",
    "preview": "iles preview --open --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "rules:dev": "vuepress dev rules",
    "rules:build": "vuepress build rules",
    "rulebook:dev": "concurrently 'npm run rulebook:run' 'npm run rulebook:css:watch'",
    "rulebook:css:watch": "postcss ./rulebook/input.postcss -o ./rulebook/style.css -w",
    "rulebook:serve:watch": "nodemon -w rules -w rulebook -e js,md,html,css rulebook/server.js",
    "rulebook:pdf:watch": "nodemon -w rules -w rulebook -e js,md,html,css rulebook/pdf.js",
    "rulebook:run": "start-server-and-test rulebook:serve:watch http://localhost:3000 rulebook:pdf:watch",
    "rulebook:css": "postcss ./rulebook/input.postcss -o ./rulebook/style.css",
    "rulebook:serve": "node rulebook/server.js",
    "rulebook:pdf": "node rulebook/pdf.js",
    "rulebook:gen": "npm run rulebook:css && start-server-and-test rulebook:serve http://localhost:3000 rulebook:pdf",
    "game:dev": "vite --config game.config.js",
    "game:build": "cp play.html index.html && vite build --config game.config.js && rm index.html",
    "gameserver:dev": "nodemon ./game/server/server.js",
    "gameserver": "node ./game/server/server.js"
  },
  "engines": {
    "node": "^14.18 || >= 16.0.0"
  },
  "devDependencies": {
    "@originjs/vite-plugin-content": "^1.0.1",
    "@tailwindcss/typography": "^0.5.0",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vuepress/bundler-vite": "^2.0.0-beta.45",
    "@vuepress/plugin-register-components": "^2.0.0-beta.51",
    "@vuepress/plugin-search": "^2.0.0-beta.51",
    "autoprefixer": "^10.4.12",
    "elm-tooling": "^1.7.0",
    "express": "^4.18.2",
    "html-pdf-node": "^1.0.8",
    "iles": "^0.8.5",
    "markdown-it": "^13.0.1",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.18",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.5",
    "vite": "^2.7.13",
    "vite-plugin-elm": "^2.5.1",
    "vue-tsc": "^0.29",
    "vuepress": "^2.0.0-beta.51"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "@vueuse/core": "^7.5.5",
    "csv": "^6.2.0",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.0",
    "elm-vue-bridge": "^1.2.3",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "tiny-emitter": "^2.1.0",
    "vue": "^3.2.27",
    "vuedraggable": "^4.1.0"
  }
}