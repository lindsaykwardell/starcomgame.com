{
  "name": "starcomgame.com",
  "version": "0.9.4",
  "scripts": {
    "postinstall": "elm-tooling install",
    "dev": "iles dev --open",
    "build": "iles build",
    "prebuild": "elm-tooling install",
    "preview": "iles preview --open --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "rules:dev": "vuepress dev rules",
    "rules:build": "vuepress build rules",
    "rules:pdf": "pandoc -V geometry:paperwidth=5in -V geometry:paperheight=8in rules/guide/welcome.md rules/guide/play-area.md rules/guide/card-types.md rules/guide/domains.md rules/play/overview.md rules/play/ready.md rules/play/command.md rules/play/combat.md rules/play/end.md rules/etc/additional-rules.md --output rules/.vuepress/public/star_commander_rulebook.pdf",
    "game:dev": "vite --config game.config.js",
    "game:build": "cp play.html index.html && vite build --config game.config.js && rm index.html",
    "gameserver:dev": "nodemon ./game/server/server.js",
    "gameserver": "node ./game/server/server.js"
  },
  "engines": {
    "node": "^14.18 || >= 16.0.0"
  },
  "devDependencies": {
    "@originjs/vite-plugin-content": "^1.0.1",
    "@tailwindcss/typography": "^0.5.0",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vuepress/bundler-vite": "^2.0.0-beta.27",
    "@vuepress/plugin-register-components": "^2.0.0-beta.36",
    "@vuepress/plugin-search": "^2.0.0-beta.27",
    "autoprefixer": "^10.4.1",
    "elm-tooling": "^1.7.0",
    "iles": "^0.7.5",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.8",
    "typescript": "^4.5",
    "vite": "^2.7.13",
    "vite-plugin-elm": "^2.5.1",
    "vue-tsc": "^0.29",
    "vuepress": "^2.0.0-beta.27"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "@vueuse/core": "^7.5.5",
    "csv": "^6.2.0",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.0",
    "elm-vue-bridge": "^1.2.3",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "tiny-emitter": "^2.1.0",
    "vue": "^3.2.27",
    "vuedraggable": "^4.1.0"
  }
}